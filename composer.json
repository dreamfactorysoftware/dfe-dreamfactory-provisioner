{
    "name":              "dreamfactory/dfe-dreamfactory-provisioner",
    "description":       "DreamFactory(tm) Enterprise DreamFactory Provisioner Package",
    "homepage":          "https://www.dreamfactory.com/",
    "keywords":          [
        "dreamfactory",
        "dfe",
        "instance",
        "provisioner"
    ],
    "license":           [
        "proprietary"
    ],
    "authors":           [
        {
            "name":  "Jerry Ablan",
            "email": "jerryablan@dreamfactory.com"
        }
    ],
    "repositories":      [
        {
            "type": "vcs",
            "url":  "https://github.com/dreamfactorysoftware/dfe-database.git"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/dreamfactorysoftware/dfe-capsule.git"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/dreamfactorysoftware/dfe-storage.git"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/dreamfactorysoftware/dfe-common.git"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/dreamfactorysoftware/dfe-console.git"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/dreamfactorysoftware/dfe-audit.git"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/dreamfactorysoftware/dfe-partner.git"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/dreamfactorysoftware/dfe-api-client.git"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/dreamfactorysoftware/dfe-ops-client.git"
        }
    ],
    "prefer-stable":     false,
    "minimum-stability": "dev",
    "require":           {
        "dreamfactory/dfe-storage":              "dev-feature/l52-upgrade as dev-master",
        "dreamfactory/dfe-database":             "dev-feature/l52-upgrade as dev-master",
        "dreamfactory/dfe-common":               "dev-feature/l52-upgrade as dev-master",
        "graham-campbell/flysystem":             "^3.0",
        "league/flysystem-ziparchive":           "~1.0",
        "league/flysystem-eventable-filesystem": "~1.0",
        "dreamfactory/php-utils":                "1.1.*"
    },
    "require-dev":       {
        "laravel/framework": "5.2.*",
        "phpunit/phpunit":   "@stable"
    },
    "autoload":          {
        "psr-4": {
            "DreamFactory\\Enterprise\\Provisioners\\DreamFactory\\": "src/"
        }
    },
    "config":            {
        "preferred-install": "dist"
    },
    "extra":             {
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        }
    }
}
